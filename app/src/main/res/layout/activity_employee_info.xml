<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/_15sdp"
    tools:context="com.example.sparken02.retrofitemployee.EmployeeInfo">

    <TextView
        android:id="@+id/txtInfoname"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
         android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtInfoemail"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtInfopassword"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtInfomobile"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtInfogender"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtInfocity"
        android:textColor="@android:color/black"
        android:textSize="@dimen/_15sdp"
        android:paddingBottom="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
